<div class="container col-md-10">
    <div class="row">
        <div class="col-md-5">
            <img src="{{student.image}}" width="300", height="300" alt="student image">
        </div>
        <div class="col-md-7">
            <h1>Student Infor</h1>
            <h3>Name: {{ student.name }}</h3>
            <h3>GenDer: {{ student.gender }}</h3>
            <h3>Date: {{ dateFormat student.dob "DD-MM-YYYY" }}</h3>
            <h3>GPA: {{ student.gpa }}</h3>
        </div>
    </div>
</div>